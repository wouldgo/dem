{
  "sessionExpiration": 86400,
  "appName": "visual-dem",
  "mongoIpAddr": "127.0.0.1",
  "amqp": {
    "exchangeName": "dem:points",
    "host": "amqp://localhost"
  },
  "maxFileSize": 1000000000,
  "httpConf": {
    "host": "0.0.0.0",
    "port": 3000,
    "routes": {
      "cors": {
        "origin": [
          "*"
        ],
        "credentials": true,
        "headers": [
          "Origin",
          "Accept",
          "Content-Type",
          "Content-Length",
          "Authorization",
          "If-None-Match"
        ],
        "additionalHeaders": [
          "X-Requested-With",
          "X-HTTP-Method-Override"
        ]
      }
    }
  }
}
